        "# Using function empty_or_full for checking  if the parking lane is empty or not.\n",
        "\n",
        "def empty_or_full(parking_lane, capacity):\n",
        "    num_cars = len(parking_lane)\n",
        "    if num_cars == 0:\n",
        "        return \"empty\"\n",
        "    elif num_cars == capacity:\n",
        "        return \"full\"\n",
        "    else:\n",
        "        return \"neither\"\n",
        "\n",
        "# Example usage\n",
        "# print(empty_or_full(['RTY-5655', 'FF 22', 'LKJ-7250'], 3))\n",
        "      # Expected Output is Full\n",
        "# print(empty_or_full([], 1))\n",
        "      # Expected Output is empty\n",
        "# print(empty_or_full(['RTY-5655'], 2))\n",
        "      # Expected Output is neither\n",
        "\n",
        "# Using function park_cars to place more cars from cars_to_park into parking_lane, without exceeding capacity.\n",
        "\n",
        "def park_cars(parking_lane, capacity, cars_to_park):\n",
        "    current_count = len(parking_lane)\n",
        "    available_space = capacity - current_count\n",
        "    # Park as many cars as possible without exceeding capacity\n",
        "    cars_to_add = cars_to_park[:available_space]  # Limit to available space\n",
        "    return parking_lane + cars_to_add  # Preserve original ordering\n",
        "\n",
        "# Example usage\n",
        "# print(park_cars(['RTY-5655'], 2, ['FF 22', 'LKJ-7250']))\n",
        "      # Expected Output is ['RTY-5655', 'FF 22']\n",
        "# print(park_cars(['RTY-5655'], 1, ['FF 22', 'LKJ-7250']))\n",
        "      # Expected Output is ['RTY-5655']\n",
        "# print(park_cars(['RTY-5655'], 2, []))\n",
        "      # Expected Output is ['RTY-5655']\n",
        "\n",
        "# Using function retrieve_cars to remove from parking_lane any cars that are in the list cars_to_retrieve.\n",
        "\n",
        "def retrieve_cars (parking_lane, cars_to_retrieve):\n",
        "    z = parking_lane\n",
        "    for i in cars_to_retrieve:\n",
        "        if i in parking_lane:\n",
        "            z.remove(i)\n",
        "    return z\n",
        "\n",
        "# Example usage\n",
        "# print(retrieve_cars(['FF 22', 'LKJ-7250'], ['RTY-5655']))\n",
        "      # Expected Output is ['FF 22', 'LKJ-7250']\n",
        "# print(retrieve_cars(['RTY-5655'], ['FF 22', 'LKJ-7250']))\n",
        "      # Expected Output is ['RTY-5655']\n",
        "# print(retrieve_cars(['RTY-5655'], []))\n",
        "      # Expected Output is ['RTY-5655']\n",
        "# print(retrieve_cars(['RTY-5655'], ['RTY-5655']))\n",
        "      # Expected Output is []\n",
        "\n",
        "# Using function check_cars to verify whether all the cars in cars_to_check are in parking_lane\n",
        "\n",
        "def check_cars (parking_lane, cars_to_check):\n",
        "    for i in cars_to_check:\n",
        "        if i not in parking_lane:\n",
        "            return False\n",
        "    return True\n",
