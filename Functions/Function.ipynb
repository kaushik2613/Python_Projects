        "def swap_to_front(parking_lane, service_lane, car):\n",
        "    # Initialising a list to store the move directions\n",
        "    directions = []\n",
        "\n",
        "    # Determine which lane contains the car and where the bubble is (the empty spot)\n",
        "    if car in parking_lane:\n",
        "        lanes = [parking_lane, service_lane]  # Store both lanes\n",
        "        if '' in parking_lane:\n",
        "            bubble_info = [0, lanes[0].index('')]  # Bubble is in parking lane\n",
        "        else:\n",
        "            bubble_info = [1, lanes[1].index('')]  # Bubble is in service lane\n",
        "    else:\n",
        "        lanes = [service_lane, parking_lane]  # Reverse order if car is in service lane\n",
        "        if '' in service_lane:\n",
        "            bubble_info = [0, lanes[0].index('')]  # Bubble is in service lane\n",
        "        else:\n",
        "            bubble_info = [1, lanes[1].index('')]  # Bubble is in parking lane\n",
        "\n",
        "    # Iterate until the car is at the front of its lane\n",
        "    while True:\n",
        "        # If the car is at the front (index 0) of its lane, exit the loop\n",
        "        if lanes[bubble_info[0]][0] == car:\n",
        "            break\n",
        "\n",
        "        # If the bubble is in the parking lane (index 0)\n",
        "        if bubble_info[0] == 0:\n",
        "            # If the bubble is at the bottom of the parking lane, switch to the service lane\n",
        "            if bubble_info[1] == len(parking_lane) - 1:\n",
        "                bubble_info[0] = 1  # Change to service lane\n",
        "                lanes[1][0], lanes[0][-1] = lanes[0][-1], lanes[1][0]  # Move the bubble to service lane\n",
        "                directions.append('O')  # Record the move\n",
        "            else:\n",
        "                # Move the bubble up in the parking lane\n",
        "                directions.append('H')  # Record the move\n",
        "                lanes[0][bubble_info[1]], lanes[0][bubble_info[1] + 1] = lanes[0][bubble_info[1] + 1], lanes[0][bubble_info[1]]\n",
        "                bubble_info[1] += 1  # Update the bubble index\n",
        "\n",
        "        # If the bubble is in the service lane (index 1)\n",
        "        else:\n",
        "            # If the bubble is at the top of the service lane, switch to parking lane\n",
        "            if bubble_info[1] == 0:\n",
        "                bubble_info[0] = 0  # Change to parking lane\n",
        "                lanes[0][0], lanes[1][0] = lanes[1][0], lanes[0][0]  # Move the bubble to parking lane\n",
        "                directions.append('O')  # Record the move\n",
        "            else:\n",
        "                # Move the bubble down in the service lane\n",
        "                directions.append('L')  # Record the move\n",
        "                lanes[1][bubble_info[1]], lanes[1][bubble_info[1] - 1] = lanes[1][bubble_info[1] - 1], lanes[1][bubble_info[1]]\n",
        "                bubble_info[1] -= 1  # Update the bubble index\n",
        "\n",
        "    return directions\n",
        "\n",
        "# # Example usage\n",
        "# print(swap_to_front(['1', '2', '3', '4'], ['5', '6', '', '8'], '3'))\n",
        "\n"
