        "def calculate_letter_grade(score):\n",
        "    if score >= 90:\n",
        "        return \"A\"\n",
        "    elif score >= 80:\n",
        "        return \"B\"\n",
        "    elif score >= 70:\n",
        "        return \"C\"\n",
        "    elif score >= 60:\n",
        "        return \"D\"\n",
        "    else:\n",
        "        return \"F\"\n",
        "\n",
        "\n",
        "def calculate_student_score(PA, mid_term_exam, final_exam):\n",
        "    # Using elif to make total marks as 0 if final marks is zero.\n",
        "    if final_exam == 0.0:\n",
        "        return calculate_letter_grade(0)                                   # or return 0 if weighted score only is required\n",
        "    # Using elif to make mid marks as final marks if mid marks is 0.\n",
        "    elif mid_term_exam == 0.0:\n",
        "        return calculate_letter_grade((PA*0.4)+(2*final_exam*0.3))         # or mid_term_exam = final_exam if weighted score only is required\n",
        "    # Printing final grade if above conditions are not satisfied.\n",
        "    return calculate_letter_grade((PA*0.4)+(mid_term_exam+final_exam)*0.3) # or weighted_score = (PA * 0.4) + (mid_term_exam * 0.3) + (final_exam * 0.3)\n",
        "                                                                           # return weighted_score if weighted score only is required\n",
        "\n",
        "# # Examples:\n",
