        "# Defining function gift_recommender to recommend a gift based on user's preference and budget\n",
        "\n",
        "def gift_recommender(preference, budget):\n",
        "  # Using elif condition for selecting preference as electronics\n",
        "    if preference == 'electronics':\n",
        "      #Using another elif for selction of gift based on budget for electronics\n",
        "        if budget <= 100:\n",
        "            return \"gaming earbuds\"\n",
        "        elif 100 < budget <= 200:\n",
        "            return \"headphone\"\n",
        "        elif budget > 200:\n",
        "            return \"smart watch\"\n",
        "  # Using elif condition for selecting preference as Clothing\n",
        "    elif preference == 'clothing':\n",
        "       #Using another elif for selction of gift based on budget for Clothing\n",
        "        if budget <= 25:\n",
        "            return \"tops\"\n",
        "        elif 25 < budget <= 50:\n",
        "            return \"jackets\"\n",
        "        elif 50 < budget <= 100:\n",
        "            return \"shoes\"\n",
        "        elif budget > 100:\n",
        "            return \"suits\"\n",
        "  # Using elif condition for selecting preference as Jewelery\n",
        "    elif preference == 'jewelry':\n",
        "       #Using another elif for selction of gift based on budget for Jewelery\n",
        "        if budget < 500:\n",
        "            return \"ring\"\n",
        "        elif 500 <= budget < 1000:\n",
        "            return \"necklace\"\n",
        "        elif budget >= 1000:\n",
        "            return \"bracelet\"\n",
        "    return \"decorative gifts!\"\n",
        "\n",
        "\n",
        "\n",
        "\n",
